{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.startupEditor": "none",
  "python.defaultInterpreterPath": "/usr/local/bin/python",
  "python.languageServer": "Pylance",
  "python.formatting.provider": "black",
  "editor.suggestSelection": "first",
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnPaste": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.fontFamily": "'Hack', 'monospace', 'Droid Sans Fallback', 'Noto Color Emoji'",
  "editor.minimap.renderCharacters": false,
  "files.autoSave": "onWindowChange",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "window.menuBarVisibility": "toggle",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "explorer.confirmDelete": false,
  "python.showStartPage": false,
  "terminal.external.linuxExec": "alacritty",
  "markdown.extension.italic.indicator": "_",
  "jupyter.alwaysTrustNotebooks": true,
  "workbench.colorTheme": "Nord",
  "remote.containers.defaultExtensions": [
    "eamodio.gitlens",
    "GitHub.copilot",
    "editorconfig.editorconfig",
    "Vtrois.gitmoji-vscode"
  ],
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash"
    }
  },
  "jupyter.allowLiveShare": false,
  "jupyter.ignoreVscodeTheme": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "jupyter.askForKernelRestart": false,
  "files.associations": {
    "*.sql": "sql"
  },
  "jupyter.allowInput": false,
  "jupyter.allowImportFromNotebook": false,
  "jupyter.addGotoCodeLenses": false,
  "jupyter.askForLargeDataFrames": false,
  "jupyter.collapseCellInputCodeByDefault": false,
  "redhat.telemetry.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.copyOnSelection": true
}
